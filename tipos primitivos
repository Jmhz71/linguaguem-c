#include <stdio.h>
#include <stdlib.h>

int main () {
    int idade = 20; //inteiro
    float peso = 80.2 ; //real
    char sexo = 'M' ; // apenas um caeacter
    char nome [200] = "Jonatas bispo santos"; //cadeia de caracteres


    printf("Idade: %d anos.\n" , idade);

    printf("Peso: %.1f kg \n", peso);
    printf("Sexo: %c \n ", sexo);
    printf("Nome: %s \n ", nome);


   return 0;
}
